<?xml version="1.0" encoding="utf-8"?>
<animated-selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="true" android:drawable="@drawable/sesl_checkbox_on_dim" />
    <item android:state_enabled="false" android:drawable="@drawable/sesl_checkbox_off_dim" />
    <item android:state_checked="true" android:id="@id/checked" android:drawable="@drawable/sesl_checkbox_on" />
    <item android:state_checked="false" android:id="@id/unchecked" android:drawable="@drawable/sesl_checkbox" />
    <transition android:drawable="@drawable/sesl_checkbox_transition" android:toId="@id/checked" android:fromId="@id/unchecked" />
    <transition android:drawable="@drawable/sesl_checkbox_transition_reverse" android:toId="@id/unchecked" android:fromId="@id/checked" />
</animated-selector>